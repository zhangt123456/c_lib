#include <stdio.h>
#include <stdlib.h>
#include <string.h>

char CSV_HEAD_en[]="序号,传输方向,第几路CAN,时间标识,增量时间,帧 ID,帧类型,帧格式,数据长度,数据,";
char data[] = "1,接收,0,14:40:51.732.452 ,00:00:09.999.240 ,0x00000721,数据帧,标准帧,08,01 70 09 01 aa aa aa aa ,";

typedef struct{
	char identity[];
	char direction[];
	char channel[];
	char time[];
	char delt_time[];
	char fid[];
	char ftype[];
	char fformat[];
	char flen[];
	char fdata[];
}raw;

raw rawData;

const char *delim = ",";
char *token;

int main(void) {

	token = strtok(CSV_HEAD_en,delim);
	while(token != null){
		printf("%s\n",token);
		token = strtok(null,delim);
	}

	return EXIT_SUCCESS;
}
