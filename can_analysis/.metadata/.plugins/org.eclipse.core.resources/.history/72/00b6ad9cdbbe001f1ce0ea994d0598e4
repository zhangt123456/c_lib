#ifndef HAL_USER_H_
#define HAL_USER_H_

typedef struct{
	char name[32];
	char passworld[16];

	char name_md5[64];
	char password_md5[64];
}USER_INPUT;

typedef struct{
	char id[4];
	char group[16];
	char real_name[16];
	char name_md5[64];
	char passworld_md5[64];
}user_info;



#endif /* HAL_USER_H_ */
