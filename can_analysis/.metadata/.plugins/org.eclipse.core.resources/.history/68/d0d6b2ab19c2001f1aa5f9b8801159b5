#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#include "string2digit.h"

char len[] = "08";
char num[] = "123456789aBcdef01234";
char s[] = " dh jf dks ljd k ";



int main(void) {


	printf("sizeof(u8) = %llu\n",sizeof(u8));
	printf("sizeof(u16) = %llu\n",sizeof(u16));
	printf("sizeof(u32) = %llu\n",sizeof(u32));
	printf("sizeof(u64) = %llu\n",sizeof(u64));
	printf("\n");

	printf("len = %d\n",string2byte(len));
	printf("len = 0x%x\n",string2byte(len));
	printf("\n");

	printf("num = %lld\n",string2hex(num));
	printf("num = 0x%x\n",(int)string2hex(num));
	printf("\n");

	u8 v[8]={0};
	for(int i=0;i<8;i++)
		printf("%x, ",v[i]);
	printf("\n\n");

	string2array(num , v);

	for(int i=0;i<8;i++)
		printf("%x, ",v[i]);
	printf("\n\n");

	printf("s= |%s|\n", s);
	removeSpace(s,' ');
	printf("s= |%s|\n", s);

	system("pause");
	return EXIT_SUCCESS;
}



